{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Skill(commit)",
      "Skill(commit:*)",
      "WebFetch(domain:docs.claude.com)",
      "WebFetch(domain:pub.dev)",
      "WebFetch(domain:github.com)"
    ]
  },
  "teammateMode": "tmux",
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "FILE=$(jq -r '.tool_input.file_path') && if [[ \"$FILE\" == *.dart ]]; then dart format \"$FILE\" && echo \"dart format completed: $FILE\"; fi"
          }
        ]
      }
    ]
  }
}
